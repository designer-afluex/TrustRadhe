                                                        </div>
                                                    </div>
                                                    <div class="col-lg-3">
                                                        <div class="form-group">
                                                            <label>Status  </label>
                                                            @Html.DropDownListFor(m => m.Status, new SelectList(ViewBag.PaidStatus, "Value", "Text"), new { @class = "form-control" })
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-3">
                                                        <br />
                                                        <input class="btn btn-success" type="submit" value="Search" name="Search" id="btnSearch" />
                                                    </div>
                                                </div>
