@model TrustRadhe.Models.KYCDocuments

@{
    ViewBag.Title = "KYCDocuments";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<!-- /.row -->
@using (Html.BeginForm("KYCDocuments", "KYCDocuments", FormMethod.Post, new { enctype = "multipart/form-data" }))
{

    <!-- PAGE-HEADER -->
    <div class="page-header">
        <div>
            <h1 class="page-title">Profile</h1>
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="/User/AssociateDashBoard">Dashboard</a></li>
                <li class="breadcrumb-item active" aria-current="page">Upload KYC</li>
            </ol>
        </div>
    </div>
        <!-- PAGE-HEADER END -->
    
            <span style="color:red">@TempData["DocumentUpload"]</span>
            <div class="row">
                <div class="col-lg-12">
                    <div class="card">

                        <div class="card-body">

                            <fieldset>
                                <div class="row">
                                    <div class="col-lg-4">
                                        <div class="form-group">
                                            <label>Adhar Number <span style="color:red">*</span></label>
                                            @Html.TextBoxFor(m => m.AdharNumber, new { @class = "form-control", @required = "required" })
                                            @Html.DisplayFor(m => m.AdharStatus, new { @class = "form-control" })
                                            @Html.ValidationMessageFor(m => m.AdharNumber)
                                        </div>
                                    </div>
                                    <div class="col-lg-4">
                                        <div class="form-group">
                                            <label>Pan Number <span style="color:red">*</span></label>
                                            @Html.TextBoxFor(m => m.PanNumber, new { @class = "form-control", @required = "required" })
                                            @Html.DisplayFor(m => m.PanStatus, new { @class = "form-control" })
                                            @Html.ValidationMessageFor(m => m.PanNumber)
                                            @*<input type ="text" id="txtLastName" value="@item1.LastName" disabled="disabled" class="form-control" />*@
                                        </div>
                                    </div>
                                    <div class="col-lg-4">
                                        <div class="form-group">
                                            <label>Account No<span style="color:red">*</span></label>
                                            @Html.TextBoxFor(m => m.MemberAccNo, new { @class = "form-control", @required = "required" })
                                            @Html.DisplayFor(m => m.MemberAccNo, new { @class = "form-control" })
                                            @Html.ValidationMessageFor(m => m.MemberAccNo)
                                            @*<input type ="text" id="txtLastName" value="@item1.LastName" disabled="disabled" class="form-control" />*@
                                        </div>
                                    </div>
                                    @*<div class="col-lg-4">
                                            <div class="form-group">
                                                <label>Document Number <span style="color:red">*</span></label>
                                                @Html.TextBoxFor(m => m.DocumentNumber, new { @class = "form-control", @required = "required" })
                                                @Html.DisplayFor(m => m.DocumentStatus, new { @class = "form-control" })
                                                @Html.ValidationMessageFor(m => m.DocumentNumber)
                                                @*<input type ="text" id="txtMobile"  value="@item1.Mobile" disabled="disabled" class="form-control" />*
                                            </div>
                                        </div>*@
                                </div>

                                <div class="row">
                                    <div class="col-lg-4">
                                        <div class="form-group">
                                            <label>IFSC Code<span style="color:red">*</span></label>
                                            @Html.TextBoxFor(m => m.IFSCCode, new { @class = "form-control", @required = "required" })
                                            @Html.DisplayFor(m => m.IFSCCode, new { @class = "form-control" })
                                            @Html.ValidationMessageFor(m => m.IFSCCode)
                                        </div>
                                    </div>
                                    <div class="col-lg-4">
                                        <div class="form-group">
                                            <label>Bank Name<span style="color:red">*</span></label>
                                            @Html.TextBoxFor(m => m.MemberBankName, new { @class = "form-control", @required = "required" })
                                            @Html.DisplayFor(m => m.MemberBankName, new { @class = "form-control" })
                                            @Html.ValidationMessageFor(m => m.MemberBankName)
                                            @*<input type ="text" id="txtLastName" value="@item1.LastName" disabled="disabled" class="form-control" />*@
                                        </div>
                                    </div>
                                    <div class="col-lg-4">
                                        <div class="form-group">
                                            <label>Branch Name<span style="color:red">*</span></label>
                                            @Html.TextBoxFor(m => m.MemberBranch, new { @class = "form-control", @required = "required" })
                                            @Html.DisplayFor(m => m.MemberBranch, new { @class = "form-control" })
                                            @Html.ValidationMessageFor(m => m.MemberBranch)
                                            @*<input type ="text" id="txtLastName" value="@item1.LastName" disabled="disabled" class="form-control" />*@
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-lg-4">
                                        <div class="form-group">
                                            <label>Adhar Image <span style="color:red">*</span></label>
                                            <input type="file" name="postedFile" required="required" />
                                            <br />
                                            <img src="@Url.Content(Model.AdharImage)" height="120" width="180" />
                                        </div>
                                    </div>
                                    <div class="col-lg-4">
                                        <div class="form-group">
                                            <label>Pan Image <span style="color:red">*</span></label>
                                            <input type="file" name="postedFile" required="required" />
                                            <br />
                                            <img src="@Url.Content(Model.PanImage)" height="120" width="180" />
                                        </div>
                                    </div>
                                    <div class="col-lg-4">
                                        <div class="form-group">
                                            <label>Passbook/Cheque Image <span style="color:red">*</span></label>
                                            <input type="file" name="postedFile" required="required" />
                                            <br />
                                            <img src="@Url.Content(Model.DocumentImage)" height="120" width="180" />
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-lg-12">
                                        <div class="form-group">
                                            <input type="submit" id="btnUpdate" class="btn btn-success" name="btnUpdate" value="Update" />
                                        </div>
                                    </div>
                                </div>
                            </fieldset>

                        </div>
                    </div>
                </div>
            </div>
}

