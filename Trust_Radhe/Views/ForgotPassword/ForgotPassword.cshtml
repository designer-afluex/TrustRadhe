        <section class="custom-padding about-us">
            <div class="container">
                <div class="row">
                    <div class="col-md-3"></div>
                    <div class="col-md-6" style="border:solid 1px">

                        @using (Html.BeginForm("ForgotPassword", "ForgotPassword", FormMethod.Post))
                        {
                            <div class="form-group text-center">
                                <h3>Recover your Password</h3>
                            </div>
                            <div class="form-group">
                                <label class="label">Username</label>
                                    @Html.TextBoxFor(m => m.LoginID, new { @class = "form-control", @required = "required" })
                                    <div class="input-group-append">
                                        <span class="input-group-text">
                                            <i class="mdi mdi-check-circle-outline"></i>
                                        </span>
                                    </div>
                            </div>
                            <div class="form-group">
                                <label class="label">Password</label>
                                    @Html.TextBoxFor(m => m.MobileNumber, new { @class = "form-control", @required = "required" })

                                    <div class="input-group-append">
                                        <span class="input-group-text">
                                            <i class="mdi mdi-check-circle-outline"></i>
                                        </span>
                                    </div>
                            </div>
                                <div class="form-group">
                                    <input type="submit" id="btnGetPassword" class="btn btn-primary submit-btn btn-block" name="btnGetPassword" value="Get Password" />
                                </div>
                                <div class="form-group d-flex justify-content-between">
                                    <div class="form-check form-check-flat mt-0">
                                        <label class="form-check-label">

                                        </label>
                                    </div>
                                    <a href="/Home/Login" class="text-small forgot-password text-black">Back To Login</a>
                                </div>

                                <div class="text-block text-center my-3">
                                    <span class="text-small font-weight-semibold">Not a member ?</span>
                                    <a href="/Home/Registration" class="text-black text-small">Create new account</a>
                                </div>
                        }
                    </div>
