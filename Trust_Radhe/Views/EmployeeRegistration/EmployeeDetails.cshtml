@model TrustRadhe.Models.EmployeeRegistrations

@{
    ViewBag.Title = "EmployeeDetails";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
@using (Html.BeginForm("PayoutReport", "AdminReport", FormMethod.Post, new { @id = "form1" }))
{
    <!--app-content open-->
    <div class="app-content">
        <div class="side-app">

            <!-- PAGE-HEADER -->
            <div class="page-header">
                <div>
                    <h1 class="page-title">Permission</h1>
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="/Admin/AdminDashBoard">Dashboard</a></li>
                        <li class="breadcrumb-item active" aria-current="page"> Employee List</li>
                    </ol>
                </div>
                <div class="ms-auto pageheader-btn">
                    <a href="/EmployeeRegistration/EmployeeRegistration" class="btn btn-primary btn-icon text-white me-2">
                        <span>
                            <i class="fe fe-plus"></i>
                        </span> Employee Registration
                    </a>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12 col-md-12">
                    <div class="card">
                        <div class="card-header">
                            <h3 class="card-title">Employee List</h3>
                            <div class="card-options">
                                <a href="#" class="card-options-collapse" data-bs-toggle="card-collapse"><i class="fe fe-chevron-up"></i></a>
                                <a href="#" class="card-options-fullscreen" data-bs-toggle="card-fullscreen"><i class="fe fe-maximize"></i></a>
                                <a href="#" class="card-options-remove" data-bs-toggle="card-remove"><i class="fe fe-x"></i></a>
                            </div>
                        </div>
                        <div class="card-body">
                            <div class="table-responsive">
                                <table class="table table-bordered text-nowrap border-bottom w-100" id="responsive-datatable">
                                    <thead>
                                        <tr>
                                            <th>Sr No.</th>
                                            <th>Name</th>
                                            <th> Login Id</th>
                                            <th>Qualification</th>
                                            <th>Contact No</th>
                                            <th>Email Id</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @if (Model != null)
                                        {
                                            if (Model.lstemp != null)
                                            {
                                                int i = 1;
                                                foreach (var item in Model.lstemp)
                                                {

                                                    <tr>
                                                        <td>@i</td>
                                                        <td>@Html.DisplayFor(m => item.Name)</td>
                                                        <td>@Html.DisplayFor(m => item.LoginId)</td>
                                                        <td>@Html.DisplayFor(m => item.EducationQualififcation)</td>

                                                        <td>@Html.DisplayFor(m => item.Mobile)</td>
                                                        <td>@Html.DisplayFor(m => item.Email)</td>

                                                    </tr>


                                                    i++;
                                                }
                                            }
                                        }
                                    </tbody>

                                </table>

                            </div>
                        </div>
                    </div>
                </div>


            </div>
        </div>
