@model TrustRadhe.Models.Home
@{
    Layout =null;
}
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
<link href="/admin_css/css/style.css" rel="stylesheet" />
<style>
    body{
        background:#F0F0F0
    }
</style>
<section class="pt-5">
    <div class="container">
        <div class="row text-center">
            <div class="col-md-8 offset-lg-2">
                <div class="card">
                    <div class="card-body">
                        <img src="/WebsiteCss/images/radhekunj_logo.png" style="height:140px" class="img-fluid"/><br/>
                        <img src="~/WebsiteCss/images/check.jpg" style="height:100px" class="img-fluid"/>
                        <h3 class="mt-1">Dear, @Session["DisplayName"]</h3>
                        <p style="font-size:20px;color:#5C5C5C;text-align:center">
                            Thank you for your registration. Now you are Trust Radhe Associate. Your details are as Below:
                            <br />
                            <span class="border-box">Login ID: @Session["LoginId"]</span>
                            <span class="border-box">Password:  @Session["Password"]</span>
                        </p>
                        <a href="/Home/Login" class="btn btn-info"> Back To Login </a>
                        <a href="/Home/Registration" class="btn btn-success"> Add New</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
  

