@model TrustRadhe.Models.Reports
@{
 ViewBag.Title = "DCMI Report";
    Layout = "~/Views/Shared/_Layout.cshtml";
}




@using (Html.BeginForm("DCMIReport", "UserReports", FormMethod.Post, new { @id = "form1" }))
{
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="section-header">
                    <div class="row">
                        <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12">
                            <div class="section-header-breadcrumb-content">
                                <h1>DCMI Report</h1>
                            </div>
                        </div>

                    </div>
                </div>
                <!-- /.panel-heading -->
                <div class="panel-body">
                    <div class="row">
                        <div class="col-md-12 table-responsive">
                           
                            <table id="example23" class="display nowrap table table-hover table-striped table-bordered" cellspacing="0" width="100%">
                                <thead>
                                    <tr>
                                        <th>Sr No.</th>

                                        <th>Login Id</th>
                                        <th>Name</th>
                                        @*<th>Password</th>*@
                                        <th hidden="hidden">Pk_DCMIId</th>
                                        <th>Month</th>
                                        <th>Total Matching</th>
                                        <th>DCMI Gross Income</th>
                                        <th>TDS</th>
                                        <th>Admin Charge</th>
                                        <th>DCMI Income</th>
                                        <th>Transaction Date</th>

                                       
                                    </tr>
                                </thead>
                                <tbody>
                                    @if (Model != null)
                                    {
                                        if (Model.lstdcmireport != null)
                                        {
                                            int i = 1;
                                            foreach (var Item in Model.lstdcmireport)
                                            {
                                        <tr class="text-left">
                                            <td>@i</td>

                                            <td>@Item.LoginId</td>
                                            <td>@Item.Name</td>
                                            @*<td>@Item.Password</td>*@
                                            <td hidden="hidden">@Item.Pk_DCMIId</td>
                                            <td>@Item.Month</td>
                                            <td>@Item.TotalMatching</td>
                                            <td>@Item.DCMIGrossIncome</td>
                                            <td>@Item.TDS</td>
                                            <td>@Item.AdminCharge</td>
                                            <td>@Item.DCMIIncome</td>
                                            <td>@Item.TransactionDate</td>
                                        </tr>
                                                i++;
                                            }

                                        }
                                    }

                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- content-wrapper ends -->
        <!-- partial:../../partials/_footer.html -->
        <!-- partial -->
    </div>

}

<script src="../vendor/jquery/jquery.min.js"></script>

<script src="//code.jquery.com/ui/1.11.4/jquery-ui.js"></script>
<link rel="stylesheet" type="text/css" href="https://code.jquery.com/ui/1.12.0/themes/smoothness/jquery-ui.css">
<script>

    $('#TransactionDate').datepicker({
        dateFormat: 'yy-mm-dd',
    });
</script>


