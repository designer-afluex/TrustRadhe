@model TrustRadhe.Models.Reports

@{
    ViewBag.Title = "BoosterLedger";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm("ViewProfile", "User", FormMethod.Post, new { @id = "form1", enctype = "multipart/form-data" }))
{

    <span style="color:red"> @TempData["UpdateProfile"]</span>
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    View Profile

                </div>
                <div class="panel-body">

                    <fieldset>
                        <div class="row">
                            <div class="col-12">
                                <div class="card">
                                    <div class="card-body">
                                        <div class="table-responsive m-t-10">
                                            <table id="example23" class="display nowrap table table-hover table-striped table-bordered" cellspacing="0" width="100%">
                                                <thead>
                                                    <tr>
                                                        <th>Sr No.</th>
                                                        <th>Maturity Date</th>
                                                        <th>Payment Date</th>
                                                        <th>Gross Amount</th>
                                                        <th>TDS</th>
                                                        <th>Processing Charge</th>
                                                        <th>Net Amount</th>
                                                        <th>Status</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    @if (Model != null)
                                                    {
                                                        if (Model.lstVoucherLedger != null)
                                                        {
                                                            int i = 1;
                                                            foreach (var Item in Model.lstVoucherLedger)
                                                            {
                                                                <tr class="text-left">
                                                                    <td>
                                                                        @i
                                                                    </td>
                                                                    <td>@Item.MaturityDate</td>
                                                                    <td>@Item.UpgradtionDate</td>
                                                                    <td>@Item.GrossAmount</td>
                                                                    <td>@Item.TDSAmount</td>
                                                                    <td>@Item.ProcessingFee</td>
                                                                    <td>@Item.Amount</td>
                                                                    <td>@Item.Status</td>
                                                                </tr>
                                                                i++;
                                                            }

                                                        }
                                                    }
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>

                            </div>
                        </div>
                    </fieldset>
                </div>
            </div>
        </div>
    </div>

}
